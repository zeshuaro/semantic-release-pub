{
  "name": "semantic-release-pub",
  "version": "0.11.9",
  "description": "semantic-release plugin to publish a Dart or Flutter package",
  "main": "dist/index.js",
  "author": "zeshuaro@gmail.com",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/zeshuaro/semantic-release-pub",
  "repository": {
    "type": "git",
    "url": "https://github.com/zeshuaro/semantic-release-pub"
  },
  "keywords": [
    "dart",
    "flutter",
    "publish",
    "registry",
    "semantic-release",
    "version"
  ],
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run tests --coverage",
    "lint": "biome check src tests && tsc --noEmit && tsc -p tsconfig.test.json",
    "lint:write": "biome check src tests --write"
  },
  "engines": {
    "node": ">=24.13.0"
  },
  "dependencies": {
    "@actions/core": "^2.0.3",
    "@semantic-release/error": "^4.0.0",
    "execa": "^9.6.1",
    "google-auth-library": "^10.5.0",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@tsconfig/node-ts": "^23.6.2",
    "@tsconfig/node24": "^24.0.4",
    "@tsconfig/recommended": "^1.0.13",
    "@tsconfig/strictest": "^2.0.8",
    "@types/common-tags": "1.8.4",
    "@types/semantic-release__error": "3.0.3",
    "@types/signale": "1.4.7",
    "@vitest/coverage-v8": "3.2.4",
    "common-tags": "1.8.2",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "semantic-release": "25.0.3",
    "typescript": "5.9.3",
    "vitest": "3.2.4",
    "vitest-mock-extended": "3.1.0"
  }
}
